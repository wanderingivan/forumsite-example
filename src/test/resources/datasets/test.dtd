<?xml version="1.0" encoding="UTF-8"?>
<!ELEMENT dataset (
	Users*,
	ForumThread*
)>

<!ELEMENT Users EMPTY>
<!ATTLIST Users
	id CDATA #IMPLIED
	username CDATA #REQUIRED
	email CDATA #REQUIRED
	description CDATA #REQUIRED
